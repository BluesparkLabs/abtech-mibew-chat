{{#extends "_layout"}}
    {{#override "windowTitle"}}{{l10n "Error"}}{{/override}}

    {{#override "pageTitle"}}{{l10n "Error"}}{{/override}}

    {{#override "message"}}
        {{#if errors}}
            <b>{{l10n "Cannot execute:"}}</b><br/>
            <ul>
            {{#each errors}}
                <li class="error">{{{this}}}</li>
            {{/each}}
            </ul>
        {{/if}}
    {{/override}}
{{/extends}}
